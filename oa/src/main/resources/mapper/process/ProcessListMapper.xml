<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.oasys.dao.process.ProcessListMapper">
    <resultMap id="processList" type="ProcessList">
        <result property="processId" column="process_id"/>
        <result property="typeName" column="type_name"/>
        <result property="statusId" column="status_id"/>
        <result property="deeplyId" column="deeply_id"/>
        <result property="processName" column="process_name"/>
        <result property="processDes" column="process_des"/>
        <result property="applyTime" column="apply_time"/>
        <result property="isChecked" column="is_checked"/>
        <result property="startTime" column="start_time"/>
        <result property="endTime" column="end_time"/>
        <result property="procseeDays" column="procsee_days"/>
        <result property="shenuser" column="shenuser"/>
        <collection property="processUserId" ofType="cn.oasys.pojo.user.User">
            <result property="userId" column="user_id"/>
            <result property="userName" column="user_name"/>
            <result property="userTel" column="user_tel"/>
            <result property="realName" column="real_name"/>
            <result property="pinyin" column="pinyin"/>
            <result property="eamil" column="eamil"/>
            <result property="address" column="address"/>
            <result property="userEdu" column="user_edu"/>
            <result property="superman" column="superman"/>
            <result property="userSchool" column="user_school"/>
            <result property="idCard" column="user_idcard"/>
            <result property="bank" column="bank"/>
            <result property="themeSkin" column="theme_skin"/>
            <result property="birth" column="birth"/>
            <result property="userSign" column="user_sign"/>
            <result property="password" column="password"/>
            <result property="salary" column="salary"/>
            <result property="imgPath" column="img_path"/>
            <result property="hireTime" column="hire_time"/>
            <result property="isLock" column="is_lock"/>
            <result property="lastLoginIp" column="last_login_ip"/>
            <result property="lastLoginTime" column="last_login_time"/>
            <result property="modifyTime" column="modify_time"/>
            <result property="modifyUserId" column="modify_user_id"/>
            <result property="fatherId" column="father_id"/>
            <result property="holiday" column="holiday"/>
            <result property="sex" column="sex"/>
            <collection property="positionId" ofType="position">
                <result property="positionId" column="position_id"/>
                <result property="name" column="name"/>
                <result property="level" column="level"/>
                <result property="describtion" column="describtion"/>
                <result property="deptid" column="deptid"/>
            </collection>
            <collection property="deptId" ofType="dept">
                <result property="deptId" column="dept_id"/>
                <result property="deptName" column="dept_name"/>
                <result property="deptTel" column="dept_tel"/>
                <result property="deptFax" column="dept_fax"/>
                <result property="email" column="email"/>
                <result property="deptAddr" column="dept_addr"/>
                <result property="deptmanager" column="deptmanager"/>
            </collection>
            <collection property="roleId" ofType="role">
                <result property="roleId" column="role_id"/>
                <result property="roleName" column="role_name"/>
                <result property="roleValue" column="role_value"/>
            </collection>
        </collection>
        <collection property="proFileid" ofType="attachment">
            <result property="attachmentId" column="attachment_id"/>
            <result property="userId" column="user_id"/>
            <result property="attachmentName" column="attachment_name"/>
            <result property="attachmentPath" column="attachment_path"/>
            <result property="attachmentSize" column="attachment_size"/>
            <result property="attachmentType" column="attachment_type"/>
            <result property="uploadTime" column="upload_time"/>
            <result property="model" column="model"/>
            <result property="attachmentShuffix" column="attachment_shuffix"/>
        </collection>
    </resultMap>
    <insert id="insertProcess">
        INSERT INTO aoa_process_list (type_name,status_id,deeply_id,process_name,process_des,process_user_id,apply_time,is_checked,start_time,end_time,procsee_days,pro_file_id,shenuser ) VALUES( #{typeName},#{statusId},#{deeplyId},#{processName},#{processDes},#{processUserId.userId},#{applyTime},#{isChecked},#{startTime},#{endTime},#{procseeDays},#{proFileid.attachmentId},#{shenuser} )
    </insert>

    <select id="selectThisApplication" resultType="cn.oasys.pojo.ProcessList" resultMap="processList">
        select * from aoa_process_list,aoa_user,aoa_attachment_list  where aoa_process_list.pro_file_id=aoa_attachment_list.attachment_id and aoa_process_list.process_user_id=aoa_user.user_id  and   aoa_process_list.process_id=#{proId}
    </select>
    <select id="findByUserId" resultType="cn.oasys.pojo.ProcessList" resultMap="processList">
        select * from aoa_process_list as pro where pro.process_user_id=#{userId} order by pro.apply_time desc
    </select>
    <select id="selectId" resultType="cn.oasys.pojo.ProcessList" resultMap="processList">
        SELECT * FROM aoa_process_list,aoa_user,aoa_attachment_list WHERE aoa_process_list.process_user_id=aoa_user.user_id and aoa_attachment_list.attachment_id=aoa_process_list.pro_file_id and process_id = #{id}
    </select>
</mapper>