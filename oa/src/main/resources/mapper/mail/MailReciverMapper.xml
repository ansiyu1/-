    <insert id="insertMailReciver">
        INSERT INTO aoa_mail_reciver ( mail_id,mail_reciver_id,is_read,is_star,is_del) VALUES( #{mailId.mailId},#{mailReciverId.userId},#{isRead},#{isStar},#{isDel})
