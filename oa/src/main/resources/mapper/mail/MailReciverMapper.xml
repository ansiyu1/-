    <select id="findByReadAndDelAndReciverId" resultType="cn.oasys.pojo.mail.MailReciver" resultMap="mailReciver">
        select*from aoa_mail_reciver,aoa_user,aoa_in_mail_list where aoa_mail_reciver.mail_reciver_id=aoa_user.user_id and aoa_mail_reciver.mail_id=aoa_in_mail_list.mail_id and aoa_mail_reciver.is_read=#{read} and aoa_mail_reciver.is_del=#{del} and aoa_mail_reciver.mail_reciver_id=#{userId}
    </select>
