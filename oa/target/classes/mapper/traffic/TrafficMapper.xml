<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.oasys.dao.traffic.TrafficMapper">
    <resultMap id="traffic" type="traffic">
        <result property="trafficId" column="traffic_id"/>
        <result property="departTime" column="depart_time"/>
        <result property="departName" column="depart_name"/>
        <result property="reachName" column="reach_name"/>
        <result property="trafficName" column="traffic_name"/>
        <result property="seatType" column="seat_type"/>
        <result property="trafficMoney" column="traffic_money"/>
        <collection property="userName" ofType="user">
            <result property="userId" column="user_id"/>
            <result property="userName" column="user_name"/>
            <result property="userTel" column="user_tel"/>
            <result property="realName" column="real_name"/>
            <result property="pinyin" column="pinyin"/>
            <result property="eamil" column="eamil"/>
            <result property="address" column="address"/>
            <result property="userEdu" column="user_edu"/>
            <result property="superman" column="superman"/>
            <result property="userSchool" column="user_school"/>
            <result property="idCard" column="user_idcard"/>
            <result property="bank" column="bank"/>
            <result property="themeSkin" column="theme_skin"/>
            <result property="birth" column="birth"/>
            <result property="userSign" column="user_sign"/>
            <result property="password" column="password"/>
            <result property="salary" column="salary"/>
            <result property="imgPath" column="img_path"/>
            <result property="hireTime" column="hire_time"/>
            <result property="isLock" column="is_lock"/>
            <result property="lastLoginIp" column="last_login_ip"/>
            <result property="lastLoginTime" column="last_login_time"/>
            <result property="modifyTime" column="modify_time"/>
            <result property="modifyUserId" column="modify_user_id"/>
            <result property="fatherId" column="father_id"/>
            <result property="holiday" column="holiday"/>
            <result property="sex" column="sex"/>
            <collection property="positionId" ofType="position">
                <result property="positionId" column="position_id"/>
                <result property="name" column="name"/>
                <result property="level" column="level"/>
                <result property="describtion" column="describtion"/>
                <result property="deptid" column="deptid"/>
            </collection>
            <collection property="deptId" ofType="dept">
                <result property="deptId" column="dept_id"/>
                <result property="deptName" column="dept_name"/>
                <result property="deptTel" column="dept_tel"/>
                <result property="deptFax" column="dept_fax"/>
                <result property="email" column="email"/>
                <result property="deptAddr" column="dept_addr"/>
                <result property="deptmanager" column="deptmanager"/>
            </collection>
            <collection property="roleId" ofType="role">
                <result property="roleId" column="role_id"/>
                <result property="roleName" column="role_name"/>
                <result property="roleValue" column="role_value"/>
            </collection>
        </collection>
        <collection property="evectionId" ofType="eveCtIonMoney">
            <result property="evectionmoneyId" column="evectionmoney_id"/>
        </collection>
    </resultMap>
    <select id="selectTraffic" resultType="cn.oasys.pojo.traffic.Traffic" resultMap="traffic">
        SELECT * FROM aoa_traffic,aoa_user WHERE aoa_traffic.user_name=aoa_user.user_id
    </select>


</mapper>