    <insert id="insertInMail">
        INSERT INTO aoa_in_mail_list ( mail_type,mail_status_id,mail_in_push_user_id,mail_title,mail_content,in_receiver,mail_file_id,mail_create_time,mail_number_id,mail_del,mail_push,mail_star ) VALUES( #{mailType},#{mailStatusId},#{mailInPushUserId.userId},#{mailTitle},#{mailContent},#{inReceiver},#{mailFileId.attachmentId},#{mailCreateTime},#{mailNumberid.mailNumberId},#{mailDel},#{mailPush},#{mailStar} )
    </insert>
    <select id="findByPushAndDelAndMailUserid" resultType="cn.oasys.pojo.mail.Inmaillist" resultMap="inmaillist">
        select*from aoa_in_mail_list,aoa_user,aoa_attachment_list,aoa_mailnumber where aoa_in_mail_list.mail_in_push_user_id=aoa_user.user_id and aoa_attachment_list.attachment_id=aoa_in_mail_list.mail_file_id and aoa_in_mail_list.mail_number_id=aoa_mailnumber.mail_number_id and aoa_in_mail_list.mail_push=#{push} and aoa_in_mail_list.mail_del=#{del} and aoa_in_mail_list.mail_in_push_user_id=#{userId}
    </select>
    <select id="selectInMail" resultType="cn.oasys.pojo.mail.Inmaillist" resultMap="inmaillist">
        select*from aoa_in_mail_list,aoa_user,aoa_attachment_list,aoa_mailnumber where aoa_in_mail_list.mail_in_push_user_id=aoa_user.user_id and aoa_attachment_list.attachment_id=aoa_in_mail_list.mail_file_id and aoa_in_mail_list.mail_number_id=aoa_mailnumber.mail_number_id and aoa_in_mail_list.mail_id=#{lid}
    </select>
    <select id="selectMaxId" resultType="cn.oasys.pojo.mail.Inmaillist">
        SELECT * from aoa_in_mail_list where mail_id = (SELECT max(mail_id) FROM a);
    </select>

